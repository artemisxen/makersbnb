<main>
  <div class="row">
    <div class="container">
      <div class="col-sm-3"></div>
      <div class="col-sm-6">
        <div class="book-space-header">
        <h1>Book a Space</h1>
        <p>bla bla bla</p>
        <a href="/addlisting" role="button" class="btn btn-info"> List a Space</a>
      </div>
      </div>
      <div class="col-sm-3"></div>
      </div>
  </div>

  <div class="row">
    <div class="container">
      <div class="col-sm-1"></div>
      <div class="col-sm-10">
        <div class="book-space-listings">

          <% if (spaces) { %>
            <% spaces.forEach(function(space) { %>
              <div class="panel panel-default">
                <div class="media">
                   <div class="media-left">
                     <img src="<%= space.imagePath %>" class="media-object">
                   </div>
                   <div class="media-body">
                     <h4 class="media-heading"><%= space.title %></h4>
                     <p><%= space.description %></p>
                   </div>
                </div>
              </div>
            <% }) %>
           <% } %>

        </div>
      </div>
      <div class="col-sm-1"></div>
    </div>
  </div>

</main>
